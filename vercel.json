{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "src/app/api/templates/create/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/pages/route.ts": {
      "maxDuration": 20
    },
    "src/app/api/subdomains/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/subdomains/[subdomain]/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/templates",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/admin",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/templates",
      "destination": "/admin"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "regions": ["iad1"]
}